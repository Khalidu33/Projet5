
{% block body %}
    <nav class="navbar navbar-expand-lg navbar-light">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo01" aria-controls="navbarTogglerDemo01" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
            <span>Menu</span>
        </button>
        <div class="collapse navbar-collapse" id="navbarTogglerDemo01">
            <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                <li class="nav-item">
                    <a class="nav-link {% if c_menu is defined and c_menu == 'homes' %}active{% endif %}" href="{{ path('home') }}">Acceuil</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if c_menu is defined and c_menu == 'admins' %}active{% endif %}" href="{{ path('admin.index') }}">Admin</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if c_menu is defined and c_menu == 'attractions' %}active{% endif %}" href="{{ path('admin.attraction.index') }}">Attractions</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if c_menu is defined and c_menu == 'accommodations' %}active{% endif %}" href="{{ path('admin.accommodation.index') }}">Hébergements</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if c_menu is defined and c_menu == 'tickets' %}active{% endif %}" href="{{ path('admin.ticket.index') }}">Ticket</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if c_menu is defined and c_menu == 'user' %}active{% endif %}" href="{{ path('admin.user.index') }}">Utilisateur</a>
                </li>
            </ul>
            {% if app.user %}
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a href="{{ path('logout') }}" class="nav-link">Se déconnecter</a>
                    </li>
                </ul>
            {% endif %}
        </div>
    </nav>
{% endblock %}